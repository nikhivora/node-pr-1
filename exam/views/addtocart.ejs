<body>
    <style>
        tr {
             margin-top: 10px !important;
        }
    </style>
    <center>
    <h1> view Products</h1>
   
      <% Crat.map((val , i )=>{   %>

      <div class="card" style="width: 18rem;">
        <img src="../<%= val.image  %>" class="card-img-top" alt="...">
        <div class="card-body">
          <h3 class="card-title"> <%= val.name  %></h3>
          <h5 class="card-subtitle"><%= val.price  %></h5>
          <h6><%= val.qt  %></h6>
          <p class="card-text"><%= val.description  %></p>
          <button> 
            <a class="dropdown-item" href="/deletecart?id=<%= val.id %>">
            <i class="bx bx-edit-alt me-1"></i> Delete Crat
          </a></button>
        </div>
      </div>
    <% }) %>

<button><a href="/dash">view Products</a></button>

</center>

